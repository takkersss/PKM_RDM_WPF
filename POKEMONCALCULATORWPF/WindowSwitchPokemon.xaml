<Window x:Class="POKEMONCALCULATORWPF.WindowSwitchPokemon"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:POKEMONCALCULATORWPF"
        xmlns:model="clr-namespace:POKEMONCALCULATORWPF.model"
        mc:Ignorable="d"
        Title="WindowSwitchPokemon" Height="450" Width="450" ResizeMode="NoResize" Closing="Window_Closing">
    <Grid>
        <ListView x:Name="lvPokemonNameList" ItemsSource="{Binding AllPokemonName}" HorizontalAlignment="Left" Width="225" Margin="5,5,0,5" Loaded="lvPokemonNameList_Loaded" />
    </Grid>
</Window>
